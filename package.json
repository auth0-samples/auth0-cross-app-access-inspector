{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "build:server": "tsc src/server/**/*.ts --outDir dist/server --target es2022 --module esnext --moduleResolution bundler",
    "preview": "npm run build && npm start"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "highlight.js": "^11.11.1",
    "jose": "^6.1.3",
    "passport": "^0.7.0",
    "passport-openidconnect": "^0.1.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-express": "^0.21.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/passport-openidconnect": "^0.1.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "vite": "^7.2.6"
  }
}
